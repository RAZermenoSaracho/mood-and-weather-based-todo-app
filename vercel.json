{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/Scripts/(.*)",
            "dest": "/public/Scripts/$1"
        },
        {
            "src": "/Styles/(.*)",
            "dest": "/public/Styles/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}